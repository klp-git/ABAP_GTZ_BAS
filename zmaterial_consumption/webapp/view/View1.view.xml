<mvc:View controllerName="zmaterialconsumption.controller.View1"
      xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:table="sap.ui.table"
    xmlns:layout="sap.ui.layout">
   
    <Page id="page">
        <content>
            <VBox id="_IDGenVBox">
                <!-- Title -->
                <Title id="invoiceTitle" text="Material consumption Against Batch" class="customTitle" />
               
                <!-- Horizontal Line -->
                <HBox id="_IDGenHBox" class="horizontalLine"></HBox>
            </VBox>
           
            <!-- Use sap.ui.layout.Grid -->
            <layout:Grid
                id="gridLayout"
                width="100%"
                defaultSpan="L6 M6 S12"
                hSpacing="0.5"
                vSpacing="0.5">
                <layout:content>
                    <!-- Posting Date and Button -->
                    <layout:VerticalLayout id="_IDGenVerticalLayout1">
                        <HBox id="_IDGenHBox3" alignItems="Center" justifyContent="Start" class="compactHBox MarginTop">
                            <Label id="_IDGenLabel2" text="Process Order No* :" />
                            <Input id="_IDGenInput" width="200px" value="" type="Number" />
                            <Label id="_IDGenLabel3" text="Material Document No* :" />
                            <Input id="_IDGenInput1" width="200px" value="" type="Number" />
                            <Button id="_IDGenButton" text="Print" class="MarginLeft"  width="50px" press="onClickPrint" />
                        </HBox>
                    </layout:VerticalLayout>
                </layout:content>
            </layout:Grid>
        </content>
    </Page>
</mvc:View>